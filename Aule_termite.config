[options]
allow_bold = true
#audible_bell = false
clickable_url = true
#dynamic_title = true
font = Ubuntu Mono 9
#fullscreen = true
#geometry = 640x480
#icon_name = terminal
#mouse_autohide = false
#scroll_on_output = false
#scroll_on_keystroke = true
# Length of the scrollback buffer, 0 disabled the scrollback buffer
# and setting it to a negative value means "infinite scrollback"
scrollback_lines = 10000
#search_wrap = true
#urgent_on_bell = true
#hyperlinks = false

# $BROWSER is used by default if set, with xdg-open as a fallback
browser = vivaldi-stable

# "system", "on" or "off"
#cursor_blink = system

# "block", "underline" or "ibeam"
#cursor_shape = block

# Hide links that are no longer valid in url select overlay mode
#filter_unmatched_urls = true

# Emit escape sequences for extra modified keys
#modify_other_keys = false

# set size hints for the window
#size_hints = false

# "off", "left" or "right"
scrollbar = off

[colors]
# If both of these are unset, cursor falls back to the foreground color,
# and cursor_foreground falls back to the background color.
#cursor = #dcdccc
#cursor_foreground = #dcdccc

#foreground = #dcdccc
#foreground_bold = #ffffff
#background = #3f3f3f

# 20% background transparency (requires a compositor)
#background = rgba(32, 38, 23, 0.8)

# If unset, will reverse foreground and background
#highlight = #2f2f2f

# Colors from color0 to color254 can be set
#color0 = #3f3f3f
#color1 = #705050
#color2 = #60b48a
#color3 = #dfaf8f
#color4 = #506070
#color5 = #dc8cc3
#color6 = #8cd0d3
#color7 = #dcdccc
#color8 = #709080
#color9 = #dca3a3
#color10 = #c3bf9f
#color11 = #f0dfaf
#color12 = #94bff3
#color13 = #ec93d3
#color14 = #93e0e3
#color15 = #ffffff

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# vim: ft=dosini cms=#%s

[colors]
# Base16 Atelier Estuary
# Author: Bram de Haan (http://atelierbramdehaan.nl)

foreground      = #929181
foreground_bold = #e7e6df
cursor          = #e7e6df
background      = #22221b

# 16 color space

# Black, Gray, Silver, White
color0  = #22221b
color8  = #6c6b5a
color7  = #929181
color15 = #f4f3ec

# Red
color1  = #ba6236
color9  = #ba6236

# Green
color2  = #7d9726
color10 = #7d9726

# Yellow
color3  = #a5980d
color11 = #a5980d

# Blue
color4  = #36a166
color12 = #36a166

# Purple
color5  = #5f9182
color13 = #5f9182

# Teal
color6  = #5b9d48
color14 = #5b9d48

# Extra colors
color16 = #ae7313
color17 = #9d6c7c
color18 = #302f27
color19 = #5f5e4e
color20 = #878573
color21 = #e7e6df
